let name="Mg Mg";
let password="1234";
if (name=="Mg Mg" && password=="1234")
console.log("Log In Success");
else
console.log("Please Enter Correctly")


let a="/";
switch(a){
    case "+":
    console.log(1+1);
    break;

    case "-":
    console.log(1-1);
    break;
    
    case "*":
    console.log(1*1);
    break;

    case "/":
    console.log(1/1);
    break;

    case "%":
    console.log(1%1);
    break;

    default:
    console.log("Enter Number");
    break;

}
